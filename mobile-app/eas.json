{
  "cli": {
    "version": ">= 9.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "KOTLIN_VERSION": "2.1.0"
        }
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_SUPABASE_URL": "${EXPO_PUBLIC_SUPABASE_URL}",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "${EXPO_PUBLIC_SUPABASE_ANON_KEY}",
        "EXPO_PUBLIC_OPENSTATES_API_KEY": "${EXPO_PUBLIC_OPENSTATES_API_KEY}",
        "EXPO_PUBLIC_LOCATIONIQ_API_KEY": "${EXPO_PUBLIC_LOCATIONIQ_API_KEY}",
        "EXPO_PUBLIC_RECAPTCHA_SITE_KEY": "${EXPO_PUBLIC_RECAPTCHA_SITE_KEY}",
        "EXPO_PUBLIC_FIREBASE_WEB_CONFIG": "${EXPO_PUBLIC_FIREBASE_WEB_CONFIG}"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}